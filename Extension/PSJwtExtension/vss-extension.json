{
    "manifestVersion": 1,
    "id": "PSConfEU-Demo-Extension",
    "version": "0.0.0",
    "name": "PSConfEU Demo Extension",
    "description": "PSConfEU JSON Web Token Demo Extension",
    "publisher": "SLVStranger",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],
    "icons": {
        "default": "images/JWT-128.png",
        "large": "images/JWT-512.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "JSON",
        "Demo"
    ],
    "contributions": [
        {
            "id": "JWT-Demo",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "PSJwt-v1.0"
            }
        }
    ],
    "content": {
        "details": {
            "path": "Marketplace.md"
        }
    },
    "files": [
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "PsJwt-v1.0",
            "packagePath": "PsJwt-v1.0"
        }
    ]
}