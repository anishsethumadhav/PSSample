{
    "manifestVersion": 1,
    "id": "PSAnish-Demo-Extension",
    "version": "0.0.0",
    "name": "PSAnish Demo Extension",
    "description": "PSAnish Demo Extension",
    "publisher": "Mossad",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services.Cloud"
        }
    ],
    "icons": {
        "default": "images/JWT-128.png",
        "large": "images/JWT-512.png"
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "PS",
        "Demo"
    ],
    "contributions": [
        {
            "id": "PSAnish-Demo",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "PSsamp_v1.0"
            }
        }
    ],
    "content": {
        "details": {
            "path": "Marketplace.md"
        }
    },
    "files": [
        {
            "path": "node_modules/vss-web-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        },
        {
            "path": "PSsamp_v1.0",
            "packagePath": "PSsamp_v1.0"
        }
    ]
}
